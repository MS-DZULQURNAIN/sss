{
  "name": "Bot pengambilan string melalui telegram",
  "description": "Dibuat oleh MS-DZULQURNAIN https://t.me/MSDQQQ",
  "logo": "",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
  "repository": "https://github.com/MS-DZULQURNAIN/string",
  "env": {
    "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "Obtain a Telegram bot token by contacting @BotFather",
      "required": true,
      "value": ""
    },
    "MONGO_URI": {
      "description": "database mongo db",
      "required": true,
      "value": ""
    },
    "DB_NAME": {
      "description": "nama database",
      "required": true,
      "value": ""
    }, 
    "MUST_JOIN": {
      "description": "Add username/id of a channel/group",
      "required": false,
      "value": ""
    }
  }
}
